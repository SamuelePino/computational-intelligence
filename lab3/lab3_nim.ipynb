      "source": [
        "def nim_sum(rows: list) -> int:\n",
        "    # List XOR\n",
        "    # Using reduce() + lambda + \"^\" operator\n",
        "    res = functools.reduce(lambda x, y: x ^ y, rows)\n",
        "    return res"
      ]
